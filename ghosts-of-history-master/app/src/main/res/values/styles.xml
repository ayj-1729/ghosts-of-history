<resources>
  <style name="AppButtonTheme" parent="Widget.AppCompat.Button.Colored">
    <item name="colorButtonNormal">#1A73E8</item>
    <item name="android:textAllCaps">false</item>
  </style>

  <style name="edit_preview_style" parent="">
    <item name="cornerFamily">rounded</item>
    <item name="cornerSize">20%</item>
  </style>

  <style name="preview_title" parent="TextAppearance.Material3.TitleMedium"/>

  <style name="link_title" parent="TextAppearance.Material3.TitleMedium">
    <item name="android:textColor">@color/md_theme_primary</item>
  </style>
  <style name="app_bar_title" parent="TextAppearance.Material3.TitleLarge">

  </style>

  <style name="item_body" parent="TextAppearance.Material3.BodyMedium"/>

  <style name="SplashTheme" parent="AppTheme">
    <item name="android:background">@drawable/goh_splashscreen_bg</item>
  </style>

  <style name="PreSplashScreenTheme" parent="Theme.SplashScreen">
    <item name="windowSplashScreenBackground">@color/goh_splashscreen_bg</item>
    <item name="postSplashScreenTheme">@style/SplashTheme</item>
    <item name="windowSplashScreenAnimatedIcon">@android:color/transparent</item>
  </style>

  <style name="login_input_shape" parent="ShapeAppearance.Material3.SmallComponent">
    <item name="colorPrimary">@color/md_theme_onPrimary</item>
  </style>
</resources>
